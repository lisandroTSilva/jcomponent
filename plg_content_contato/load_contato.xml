<?xml version="1.0" encoding="utf-8"?>
<extension version="3.5" type="plugin" group="content" method="upgrade">
	<name>plg_content_load_contato</name>
	<author>Lisandro Silva</author>
	<creationDate>Abril 2020</creationDate>
	<copyright>Created by Lisandro Silva with generator-joomla3 by @diarcastro</copyright>
	<license>MIT</license>
	<authorEmail>lisandro.t.silva@gmail.com</authorEmail>
	<authorUrl></authorUrl>
	<version>0.0.1</version>
	<description>PLG_CONTENT_LOAD_CONTATO_XML_DESCRIPTION</description>
	<files>
		<filename plugin="load_contato">load_contato.php</filename>
		<folder>language</folder>
	</files>
	<updateservers>
		<server type="extension" priority="1" name="Repositório Conteúdo - plg_content_load_contato">https://raw.githubusercontent.com/lisandroTSilva/jcomponent/master/build/plg_content_contato.xml</server>
	</updateservers>

	<media folder="media" destination="plg_content_load_contato">
		<folder>js</folder>
	</media>

	<config>
		<fields name="params">
			<fieldset name="basic" addfieldpath="/plugins/content/load_contato/fields"></fieldset>
		</fields>
	</config>
</extension>
